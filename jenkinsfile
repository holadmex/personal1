pipeline 
    agent any
    tools {
        jdk 'JDK'
        maven 'Maven'
    }
    stages {
        stage ('BUILD THE APPLICATION') {
            steps {
                sh 'mvn install -Dskiptest'
            }
        }
        stage ('TEST THE APPLICATION') {
            steps {
                sh 'mvn test'
            }
        }
        stage ('UNIT TEST') {
            steps {
                sh 'mvn install -DskipUnitTest'
            }
        }
    }
    l
